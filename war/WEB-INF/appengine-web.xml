<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">

	<!--<application>sparkmuse</application>-->
	<!--<version>35</version>-->

  <application>sparkmuse-staging</application>
	<version>35</version>

  <static-files>
    <include path="/public/**.*" expiration="7d" />
  </static-files>

  <!-- used in caching -->
  <env-variables>
    <env-var name="CURRENT_VERSION_ID" value="35" />
  </env-variables>

  <admin-console>
    <page name="Appstats" url="/appstats" />
    <page name="Sparkmuse Admin" url="/admin" />
  </admin-console>

</appengine-web-app>
